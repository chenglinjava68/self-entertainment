<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:dubbo="http://dubbo.apache.org/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd http://dubbo.apache.org/schema/dubbo http://dubbo.apache.org/schema/dubbo/dubbo.xsd">

    <dubbo:application name="annotation-provider"/>
    <dubbo:registry address="zookeeper://119.3.10.221:2181"/>
<!--    dubbo.metadata-report.address=zookeeper://119.3.10.221:2181-->
    <dubbo:metadata-report address="zookeeper://119.3.10.221:2181"/>
    <dubbo:protocol name="dubbo" port="20890"/>

    <bean id="testService" class="com.self.entertainment.service.impl.TestServiceImpl"/>
    <bean id="helloService" class="com.self.entertainment.service.impl.HelloServiceImpl"/>

    <dubbo:service interface="com.self.entertainment.service.TestService" ref="testService"/>
    <dubbo:service interface="com.self.entertainment.service.HelloService" ref="helloService"/>
</beans>
